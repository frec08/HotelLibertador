            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NuevoActivity"/>
        <activity android:name=".NuevoRecepActivity"/>
        <activity android:name=".DatosActivity"/>
        <activity android:name=".DatosRecepActivity"/>
        <activity android:name=".SQLiteActivity"/>
        <activity android:name=".NosotrosActivity"/>
        <activity android:name=".TerminosActivity"/>
        <activity android:name=".CotizaActivity"/>
        <activity android:name=".ArchivoActivity"/>
        <activity android:name=".NuevoCliArch"/>
        <activity android:name=".DatosCliArchActivity"/>
    </application>

</manifest>